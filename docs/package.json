    "@codemirror/view": "^6.1.1",
    "codemirror": "^6.0.1",
    "eslint": "^8.20.0",
    "eslint-plugin-react": "^7.30.1",
    "microbundle": "^0.15.0",
    "react": "^17.0.2",
