  "type": "module",
  "source": "./src/index.js",
  "exports": {
    "require": "./static/js/docs.cjs",
    "default": "./static/js/docs.modern.js"
  },
  "main": "./static/js/docs.cjs",
  "module": "./static/js/docs.module.js",
  "unpkg": "./static/js/docs.js",
  "scripts": {
    "build": "npm run build:web",
    "build:web": "microbundle --format umd --jsx React.createElement --sourcemap false --globals react=React,react-dom=ReactDOM,react-modal=ReactModal",
    "build:web:standalone": "microbundle --format umd --jsx React.createElement --sourcemap false --define process.env.NODE_ENV=production --external None",
    "dev": "microbundle watch --format umd --no-compress --jsx React.createElement --sourcemap false --globals react=React,react-dom=ReactDOM,react-modal=ReactModal",
    "lint": "eslint src"
  },
